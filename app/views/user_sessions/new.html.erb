<h3>Login</h3>

<% form_for(@user_session) do |f| %>
  <%= f.error_messages %>

    <%= f.label :username %> <%= f.text_field :username %>
  <br/>
    <%= f.label :password %> <%= f.password_field :password %>
  <br/>
    <%= f.submit 'Create', :class=>"action_button" %>
  
<% end %>

<%= link_to 'Back', user_sessions_path %>